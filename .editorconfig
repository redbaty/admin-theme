# editorconfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
indent_style = space
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[{**.js,**.json,**.css,**.scss,**.yml,**.html,!node_modules/*,!bower_components/*}]
indent_style = space
indent_size = 2
